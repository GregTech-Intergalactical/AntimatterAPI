{
  "schemaVersion": 1,
  "id": "fabricated-forge-modeldata",
  "version": "${version}",
  "name": "Fabricated Forge Modeldata",
  "description": "Ports forge's modeldata to fabric.",
  "authors": [
    "Trinsdar"
  ],
  "contact": {
    "sources": "https://github.com/FeltMC/Fabricated-Forge-API"
  },
  "license": "LGPL-2.1-only",
  "mixins": [
    "fabricated-forge-modeldata.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_1087": [
        "net/minecraftforge/client/extensions/IForgeBakedModel"
      ],
      "net/minecraft/class_1159": [
        "net/minecraftforge/client/extensions/IForgeMatrix4f"
      ],
      "net/minecraft/class_4590": [
        "net/minecraftforge/client/extensions/IForgeTransformation"
      ],
      "net/minecraft/class_2586": [
        "net/minecraftforge/common/extensions/IForgeBlockEntityExtension"
      ]
    }
  }
}